/* skibidi */
       
       .performance .gambar {
           display: flex;
           flex-wrap: wrap;
           justify-content: center;
           gap: 2rem;
           width: 100%;
           padding: 1rem;
       }
       
       .image-container {
           width: calc(33.33% - 2rem);
           min-width: 280px;
           transition: all 0.3s ease;
           background: white;
           padding: 1rem;
           border-radius: 8px;
           display: flex;
           flex-direction: column;
           align-items: center;
           box-shadow: 0 0 15px rgba(255, 255, 255, 0.5);
       }
       
       .image-container:hover {
           transform: translateY(-5px);
       }
       
       #poster {
           width: 100%;
           aspect-ratio: 16/9;
           object-fit: cover;
           background: #e74c3c;
           margin-bottom: 1rem;
           border-radius: 4px;
           display: flex;
           align-items: center;
           justify-content: center;
           color: white;
           font-size: 2rem;
       }
       
       .image-container h1 {
           color: #333;
           font-size: clamp(1.2rem, 2vw, 1.5rem);
           margin: 0.5rem 0;
           text-align: center;
       }
       
       .image-container a {
           background: #e74c3c;
           color: white;
           padding: 0.5rem 2rem;
           border-radius: 4px;
           text-decoration: none;
           font-size: clamp(1rem, 1.5vw, 1.2rem);
           margin-top: 0.5rem;
           transition: all 0.3s ease;
       }
       
       .image-container a:hover {
           transform: scale(1.05);
           background: #d44133;
       }
       
       .performance .gambar {
           display: flex;
           flex-wrap: wrap;
           justify-content: center;
           gap: 2rem;
           width: 100%;
           padding: 1rem;
       }
       /* ... rest of your CSS stays the same ... */
       /* Responsive Design */
       
       @media screen and (max-width: 1200px) {
           .image-container {
               width: calc(50% - 2rem);
           }
       }
       
       @media screen and (max-width: 768px) {
           .image-container {
               width: 100%;
               max-width: 500px;
           }
           .performance .gambar {
               gap: 1.5rem;
               padding: 0.5rem;
           }
       }
       
       @media screen and (max-width: 480px) {
           .image-container {
               padding: 0.8rem;
           }
           .image-container a {
               padding: 0.4rem 1.5rem;
           }
       }
       
       @media screen and (min-width: 1600px) {
           .image-container {
               max-width: 500px;
           }
       }